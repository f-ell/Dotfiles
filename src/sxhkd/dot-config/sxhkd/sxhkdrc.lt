# [MODIFIER + ]*[@|!]KEYSYM
#   COMMAND

super + {F12, F11}
  bl {u,d} 5

super + shift + {F12, F11}
  bl s {100,0}

alt + b
  battery

alt + c
  dunstify -u low -h string:x-dunst-stack-tag:temp "`temp.lt`"

alt + u
  dunstify -u low -h string:x-dunst-stack-tag:load "Calculating..." \
  && dunstify -u low -h string:x-dunst-stack-tag:load "`sysload`"
