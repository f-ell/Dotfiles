# [MODIFIER + ]*[@|!]KEYSYM
#   COMMAND

super + {F12, F11}
  bl {u,d} 5

super + shift + {F12, F11}
  bl s {100,0}

mode_switch + b
  ~/Git/Scripts/source/perl/system_monitors/battery

mode_switch + c
  dunstify -u low -h string:x-dunst-stack-tag:temp \
  "`~/Git/Scripts/source/shell/system_monitors/temp.lt`"
